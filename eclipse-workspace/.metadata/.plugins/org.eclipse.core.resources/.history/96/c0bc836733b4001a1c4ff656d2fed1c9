package Parties;

public class Holidays {

public static void main(String args) {
	
	Miami miami = new Miami();
	
	
	Miami.setDrinks("vodka");
	Miami.setAlcohol("whisky");
	Miami.setBars("downtown");
	
	System.out.println(Miami.getDrinks() + Miami.getAlcohol());
	
	
	
	
}



