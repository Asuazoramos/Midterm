package Doctors;

public class Careers {


public String nursing ="nurse";
public String doctors ="appoinment";
public String Medical ="help";


public Careers(){}



public Careers(String Careers) 
{
	this.nursing="nurse";
	
	
}



public Careers(String Medical1, String Medical2, String MED) 
{
	
	
	this.nursing=Medical1;
	this.doctors=Medical2;
	this.Medical=MED;
	
}
	
	
	public void getNursing ()
	{
		System.out.println("nurse");
	}
	
	public void getDoctors ()
	{
		System.out.println("appoinment");
	}
	
	
	
	public void getMedical () {
		System.out.println("help");
	}
	
	
	
	






}
















}
